{
    "name": "honda/honda",
    "description": "",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.3",
        "honda/navigation": "^0.2.0",
        "honda/pushed-resources": "^1.1.0",
        "honda/ui": "dev-master",
        "imliam/laravel-blade-helper": "^1.4",
        "laravel/framework": "^8.51",
        "laravel/tinker": "^2.6",
        "livewire/livewire": "^2.5",
        "ryangjchandler/blade-tabler-icons": "^1.1",
        "spatie/pest-plugin-test-time": "^1.0",
        "spatie/valuestore": "^1.2.5"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "barryvdh/laravel-ide-helper": "^2.10",
        "beyondcode/laravel-query-detector": "^1.5",
        "facade/ignition": "^2.11",
        "fakerphp/faker": "^1.15",
        "friendsofphp/php-cs-fixer": "^3.0",
        "mockery/mockery": "^1.4.3",
        "nunomaduro/collision": "^5.0",
        "nunomaduro/larastan": "^0.7.12",
        "pestphp/pest": "^v1.11",
        "pestphp/pest-plugin-laravel": "^v1.1",
        "roave/security-advisories": "dev-latest"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "lint": "php-cs-fixer fix -v",
        "test:lint": "php-cs-fixer fix -v --dry-run",
        "test:types": "phpstan analyse --ansi --memory-limit=0",
        "test:unit": "pest --colors=always",
        "test": [
            "@test:lint",
            "@test:types",
            "@test:unit"
        ],
        "helpers": "@php artisan ide-helper:generate"
    }
}
